                },
                {
                    "id": "sampleCheckbox",
                    "view": "Checkbox",
                    "label": "Default",
                    "checked": "true",
                    "useNative": "true"
                },
                {
                    "id": "sampleCheckboxRNP",
                    "view": "Checkbox",
                    "label": "DefaultRNP",
                    "checked": "true"
                },
                {
                    "id": "sampleSwitch",
                    "view": "Switch",
                    "useNative": "true"
                },
                {
                    "id": "sampleSwitchRNP",
                    "view": "Switch",
                    "color": "black"
                },
                {
                    "id": "samplePicker",
                    "view": "Picker",
                    "data": [
                        "Value1",
                        "Value2"
                    ]
                },
                {
                    "id": "KeyboardAvoidingView",
