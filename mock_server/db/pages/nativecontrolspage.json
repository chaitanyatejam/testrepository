{
    "id": "nativecontrolspage",
    "enableScroll": true,
    "componentCategory": "SolutionPage",
    "components": [
        {
            "id": "nativecontrolsapp",
            "componentCategory": "Application",
            "components": [
                {
                    "id": "forms",
                    "view": "Formik",
                    "fields": [
                        {
                            "view": "TextInput",
                            "name": "ema",
                            "type": "email",
                            "label": "Email",
                            "validations": [
                                {
                                    "name": "required",
                                    "arguments": [
                                        "this email is required"
                                    ]
                                },
                                {
                                    "name": "email",
                                    "arguments": [
                                        "provide valid email"
                                    ]
                                }
                            ]
                        },
                        {
                            "view": "TextInput",
                            "name": "pass",
                            "type": "password",
                            "label": "Password",
                            "validations": [
                                {
                                    "name": "required",
                                    "arguments": [
                                        "this password is required"
                                    ]
                                },
                                {
                                    "name": "min",
                                    "arguments": [
                                        8,
                                        "min 8 characters required"
                                    ]
                                }
                            ]
                        },
                        {
                            "view": "Picker",
                            "name": "gender",
                            "label": "gender",
                            "values": [
                                {
                                    "label": "male",
                                    "value": "male"
                                },
                                {
                                    "label": "female",
                                    "value": "female"
                                }
                            ]
                        },
                        {
                            "view":"DatePicker",
                            "name":"datePicker",
                            "dateFormat":"DD/MM/YYYY"
                        },
                        {
                            "view":"TimePicker",
                            "name":"timePicker",
                            "timeFormat":"hh:mm A"
                        }
                    ]
                },
                {
                    "id": "sampleactivityindicator",
                    "view": "ActivityIndicator",
                    "useNative": "true"
                },
                {
                    "id": "sampleactivityindicatorRNP",
                    "view": "ActivityIndicator",
                    "animating": "true",
                    "color": "red",
                    "size": "small"
                },
                {
                    "id": "sampleimage",
                    "view": "Image",
                    "componentCategory": "UIControl",
                    "source": {
                        "uri": "https://facebook.github.io/react-native/docs/assets/favicon.png"
                    },
                    "style": {
                        "width": 50,
                        "height": 50
                    }
                },
                {
                    "id": "sampleimageBackground",
                    "view": "ImageBackground",
                    "componentCategory": "UIControl",
                    "source": {
                        "uri": "https://cdn.pixabay.com/photo/2015/12/03/08/50/paper-1074131_960_720.jpg"
